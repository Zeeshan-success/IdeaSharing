{
  
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
    ,{
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    
    {
      "src": "/ideas",

      "dest": "index.js"
    },
    {
      "src": "/ideas/(.*)",

      "dest": "index.js"
    },
    {
      "src": "/",

      "dest": "dist/index.html"
    },
    {
      "src": "/(.+)",

      "dest": "dist/$1"
    }
  ]
}
